<?xml version="1.0"?>
<configuration>

  <location path="Register.aspx">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>

 
  <system.web>
    <authorization>
      <deny users="?"/>
      <allow roles="Admin" />
      <allow roles="User" />
      <allow roles="Manager" />
      <allow roles="Mechanic" />      
      <deny users="*" />      
    </authorization>
  </system.web>

</configuration>
